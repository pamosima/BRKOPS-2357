            "name": "Execute Python Script",
            "title": "Build pyATS Command",
            "type": "python3.script",
            "base_type": "activity",
            "properties": {
              "action_timeout": 180,
              "continue_on_failure": false,
              "display_name": "Build pyATS Command",
              "script": "import sys\n\npyats_path = sys.argv[1] if len(sys.argv) > 1 else ''\n\n# Validate required inputs\nif not pyats_path or not pyats_path.strip():\n    raise ValueError('pyATS Scripts Path static variable is required but empty. Please configure it in the workflow.')\n\n# Build the command to run pyATS natively in a Python venv\n# Activates the venv, loads environment variables from .env, and runs pyATS\npyats_cmd = f'''cd {pyats_path} && source venv/bin/activate && export $(grep -v '^#' .env | xargs) && pyats run job job.py --no-archive --no-reporter'''\n\nprint(f\"Command built for path: {pyats_path}\")",
              "script_arguments": [
                "$workflow.definition_workflow_02RVVTSJL1E093yLsvgFIDRKHXGjTjA5uY0.static.variable_workflow_02RVVTSJL1E093yLsvgFIDRKHXGjTjA5uY9$"
              ],
              "script_queries": [
                {
                  "script_query": "pyats_cmd",
                  "script_query_name": "pyats_cmd",
                  "script_query_type": "string"
                }
              ],
              "skip_execution": false
            },
            "object_type": "definition_activity"
          },
