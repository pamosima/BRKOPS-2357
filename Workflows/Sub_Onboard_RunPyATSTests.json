        "object_type": "definition_activity",
        "actions": [
          {
            "unique_name": "definition_activity_02RVVTSJL1E093yLsvgFIDRKHXGjTjA5u07",
            "name": "Execute Python Script",
            "title": "Parse pyATS Output",
            "type": "python3.script",
            "base_type": "activity",
            "properties": {
              "action_timeout": 180,
              "continue_on_failure": false,
              "display_name": "Parse pyATS Output",
              "script": "import sys\nimport re\n\ntest_output = sys.argv[1] if len(sys.argv) > 1 else ''\nssh_succeeded = sys.argv[2].lower() == 'true' if len(sys.argv) > 2 else False\n\n# Initialize results\ntests_passed = False\ntest_summary = \"Test execution failed\"\n\nif not ssh_succeeded:\n    test_summary = \"SSH command failed to execute\"\nelif not test_output:\n    test_summary = \"No test output received\"\nelse:\n    # Parse pyATS output for test results\n    # Look for pyATS summary patterns\n    \n    # Look for pyATS Job Results summary\n    # Pattern: \"Passed: X  Passx: Y  Failed: Z\"\n    summary_match = re.search(r'Passed\\s*[:|=]\\s*(\\d+).*?Failed\\s*[:|=]\\s*(\\d+)', test_output, re.IGNORECASE | re.DOTALL)\n    \n    if summary_match:\n        passed_count = int(summary_match.group(1))\n        failed_count = int(summary_match.group(2))\n        tests_passed = (failed_count == 0 and passed_count > 0)\n        test_summary = f\"pyATS Results - Passed: {passed_count}, Failed: {failed_count}\"\n    elif 'PASSED' in test_output.upper() and 'FAILED' not in test_output.upper():\n        tests_passed = True\n        test_summary = \"All tests PASSED\"\n    elif 'FAILED' in test_output.upper():\n        tests_passed = False\n        test_summary = \"Tests FAILED - check output for details\"\n    elif 'error' in test_output.lower() or 'exception' in test_output.lower():\n        tests_passed = False\n        test_summary = \"Test execution encountered errors\"\n    else:\n        # Check for successful completion indicators\n        if 'job completed' in test_output.lower() or 'finished' in test_output.lower():\n            tests_passed = True\n            test_summary = \"Test execution completed\"\n        else:\n            test_summary = \"Unable to determine test results - check output\"\n\nprint(f\"Tests Passed: {tests_passed}, Summary: {test_summary}\")",
              "script_arguments": [
                "$workflow.definition_workflow_02RVVTSJL1E093yLsvgFIDRKHXGjTjA5uY0.local.variable_workflow_02RVVTSJL1E093yLsvgFIDRKHXGjTjA5uYB$",
                "$activity.definition_activity_02RWJ8TWGEFX13PxfSjvJ9UN8ENEt8UEFps.output.succeeded$"
              ],
              "script_queries": [
                {
                  "script_query": "tests_passed",
                  "script_query_name": "tests_passed",
                  "script_query_type": "boolean"
                },
                {
                  "script_query": "test_summary",
                  "script_query_name": "test_summary",
                  "script_query_type": "string"
                }
              ],
              "skip_execution": false
