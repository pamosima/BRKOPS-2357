      {
        "unique_name": "definition_activity_02RRHZJMOI8CJ3ylmrzHvqUm1OlFfK34PYT",
        "name": "Execute Python Script",
        "title": "Truncate GPS Coordinates",
        "type": "python3.script",
        "base_type": "activity",
        "properties": {
          "action_timeout": 180,
          "continue_on_failure": false,
          "display_name": "Truncate GPS Coordinates",
          "script": "import sys\n\n# Get raw coordinates from workflow\nlat_raw = sys.argv[1]\nlng_raw = sys.argv[2]\n\n# Convert to float and round\n# Latitude: max 6 decimal places (8 digits total for values like 47.123456)\n# Longitude: max 6 decimal places\ntry:\n    lat = round(float(lat_raw), 6)\n    lng = round(float(lng_raw), 6)\nexcept:\n    lat = 0.0\n    lng = 0.0\n\n# Format as strings with max 6 decimal places\nlat_str = f\"{lat:.6f}\"\nlng_str = f\"{lng:.6f}\"\n\nprint(f\"Latitude: {lat_str}, Longitude: {lng_str}\")",
          "script_arguments": [
            "$activity.definition_activity_02RRHZJMOI8CJ3ylmrzHvqUm1OlFfK34GPS.output.variable_workflow_02RRSDEF40TUC0oWGXLkN1TmA8lXoe3RBxV$",
            "$activity.definition_activity_02RRHZJMOI8CJ3ylmrzHvqUm1OlFfK34GPS.output.variable_workflow_02RRSDEF40TUC0oWGXLkN1TmA8lXoe3RBxW$"
          ],
          "script_queries": [
            {
              "script_query": "lat_str",
              "script_query_name": "latitude",
              "script_query_type": "string"
            },
            {
              "script_query": "lng_str",
              "script_query_name": "longitude",
              "script_query_type": "string"
            }
          ],
          "skip_execution": false
        },
        "object_type": "definition_activity"
      },
