      {
        "unique_name": "definition_activity_02RVVTSJL1E093yLsvgFIDRKHXGjTjA5u06",
        "name": "Group",
        "title": "Parse Test Results",
        "type": "logic.group",
        "base_type": "activity",
        "properties": {
          "continue_on_failure": false,
          "display_name": "Parse Test Results",
          "skip_execution": false
